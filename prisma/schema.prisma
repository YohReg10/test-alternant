generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model activity {
  id          Int    @id @default(autoincrement())
  name        String
  type        String
  location    String
  distance    String
  environment String
  sensation   Int
  url_site    String
  url_img     String
}
